module.exports = {
    name: "examplejs",
    async execute(browser, url) {
        const page = await browser.newPage();
        await page.setCookie({
            name: "adminpw",
            value: process.env.CHALL_ADMINPW || "placeholder",
            domain: process.env.CHALL_DOMAIN || "localhost:8080",
            httpOnly: true,
        });
        await page.goto(url);
        await page.waitForNetworkIdle({
            timeout: 5000,
        });
        await page.close();
    },
};
